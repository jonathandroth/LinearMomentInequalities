#!/bin/bash
# This script initiates on the Odyssey server the various permutations of the linear programming methods

## Inference holding lambda constant

#1 thetac
sbatch Matlab_parallel_script_run_lp_cs_single_theta.sh
sbatch Matlab_parallel_script_run_lp_cs_single_theta_basicmoments.sh
sbatch Matlab_parallel_script_run_lp_cs_single_theta_firmlevel_moments.sh

#3 thetacs
sbatch Matlab_parallel_script_run_lp_cs_3thetacs.sh
sbatch Matlab_parallel_script_run_lp_cs_3thetacs_basicmoments.sh
sbatch Matlab_parallel_script_run_lp_cs_3thetacs_firmlevel_moments.sh
sbatch Matlab_parallel_script_run_lp_cs_3thetacs_combined_thetag.sh

#9 thetacs
sbatch Matlab_parallel_script_run_lp_cs_multiple_thetas.sh
sbatch Matlab_parallel_script_run_lp_cs_multiple_thetas_basicmoments.sh
sbatch Matlab_parallel_script_run_lp_cs_multiple_thetas_firmlevel_moments.sh
sbatch Matlab_parallel_script_run_lp_cs_multiple_thetas_combined_thetag.sh

## Inference with lambda
sbatch Matlab_parallel_script_run_lp_lambda_inference_single_theta.sh
sbatch Matlab_parallel_script_run_lp_lambda_inference_3thetacs.sh
sbatch Matlab_parallel_script_run_lp_lambda_inference_9thetacs.sh
